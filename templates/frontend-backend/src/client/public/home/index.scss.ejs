#home-page {
    display: grid;
    height: 100%;
    place-items: center;

    code {
        color: white;
        padding: 4px 8px 0px;
        background-color: #e83e8c;
        border-radius: 6px;
        font-weight: bold;
    }

    i {
        color: #de1e1e;
        font-size: 3rem;
        vertical-align: middle;
    }
}
